<nav style="padding: 20px;">
  <p><strong>Gig Name:</strong><%= schedule.gig.try(:name) %></p>
  <p><strong> Venue Name: </strong> <%= schedule.venue.try(:name) %></p>
  <p>Schedule: <strong><%= schedule.schedule %></strong></p>
  
  <ul>
  <li><%= button_to('Show', schedule_path(schedule), method: :get)%></li>
  <li><%= button_to('Edit', edit_schedule_path(schedule), method: :get)%></li>
  <li><%= button_to('Delete', schedule_path(schedule), method: :delete, data:{confirm: 'Delete the schedule? Are you sure?'})%></li>
 </ul>
</nav>


